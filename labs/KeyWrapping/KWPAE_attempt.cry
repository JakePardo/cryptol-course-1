widen a = 0 # a

KWPAE_attempt :
    {k, l}
    (
     fin k,
     fin l,
     k > 0,
     k < 2^^32,
     l == 64 + (8*k) + (8*k) %^ 64 // type of S
    ) =>
    ([128] -> [128]) -> [8*k] -> [l]
    
KWPAE_attempt CIPHk P = C
  where
    ICV2 = 0xA65959A6
    PAD = zero
    PLEN = `k : [32]
    S = ICV2 # PLEN # P # PAD : [l]
    C = if PLEN <= 64 then
            widen (CIPHk (take S))
          else
            join (W CIPHk (split`{l/64} S))

